<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- Adult Backstories -->

<AlienRace.BackstoryDef>

    <defName>Thrumboian_Wild</defName>
    <title>Thrumbo Wanderer</title>
    <titleShort>White Horn</titleShort>
    <baseDescription>[PAWN_nameDef] has a lifetime of experience and they seem to know at least a little of just about everything. 
</baseDescription>

    <bodyTypeMale>Male</bodyTypeMale>
    <bodyTypeFemale>Female</bodyTypeFemale>

    <slot>Adulthood</slot>
    <skillGains>
      <li>
        <defName>Social</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Artistic</defName>
        <amount>2</amount>
      </li>
      <li>
        <defName>Crafting</defName>
        <amount>3</amount>
      </li>
	  <li>
        <defName>Cooking</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Intellectual</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Medicine</defName>
        <amount>3</amount>
      </li>
      <li>
        <defName>Shooting</defName>
        <amount>1</amount>
      </li>
      <li>
        <defName>Melee</defName>
        <amount>5</amount>
      </li>
	  <li>
       <defName>Mining</defName>
        <amount>3</amount>
      </li>
	  <li>
	    <defName>Plants</defName>
        <amount>5</amount>
      </li>
      <li>
        <defName>Animals</defName>
        <amount>5</amount>
      </li>
      <li>
        <defName>Construction</defName>
        <amount>2</amount>
      </li>
    </skillGains>
    
    <spawnCategories>
	 <li>ThrumboStories</li>
    </spawnCategories>
    
  </AlienRace.BackstoryDef>
  
